lemma allDefs'_disjoint: " n \<in> set (\<alpha>n g) \<Longrightarrow> m \<in> set (\<alpha>n g) \<Longrightarrow> n \<noteq> m \<Longrightarrow> (defs' g n \<union> {v. (n, v) \<in> dom (phis' g)}) \<inter> (defs' g m \<union> {v. (m, v) \<in> dom (phis' g)}) = {}"