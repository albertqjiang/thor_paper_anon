lemma injectivity_subexpr1: assumes "a \<notin> A" assumes "X \<in> P \<and> X' \<in> P'" assumes "insert (insert a X) (P - {X}) = insert (insert a X') (P' - {X'})" assumes "(partition_on A P \<and> card P = Suc k') \<and> (partition_on A P' \<and> card P' = Suc k')" shows "P = P'" and "X = X'"